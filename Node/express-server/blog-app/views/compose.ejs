<%- include("partials/header")%>

<h1>Compose</h1>
<form action="/compose" method="post">
  <div class="mb-3">
  <label for="exampleFormControlInput1" class="form-label">Title</label>
  <input type="text" class="form-control" id="exampleFormControlInput1" name="title" placeholder="Enter your title here..." autocomplete="on">
  </div>
  <div class="mb-3">
  <label for="exampleFormControlTextarea1" class="form-label">Post</label>
  <textarea class="form-control" id="exampleFormControlTextarea1" name="text" rows="3" placeholder="Enter your post here..."autocomplete="on"></textarea>
  </div>
<button type="submit" name="button">Publish</button>
</form>

<%- include("partials/footer")%>
